{
  "manifest_version": 2,
  "name": "Quick SM Launcher 1.0",
  "description": "Chrome extension for Quick Launch of my social media pages",
  "author": "Rafaela",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "content_security_policy": "script-src-elem https://kit.fontawesome.com/9b209cfb68.js; script-src 'self' 'unsafe-eval' https://use.fontawesome.com; object-src 'self'",
  "icons": {
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Quick SM Launcher"
  }
}
